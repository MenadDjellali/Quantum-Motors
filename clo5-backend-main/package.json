    "@types/babel__preset-env": "^7",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
