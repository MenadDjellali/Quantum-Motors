  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "joi": "^17.12.1",
    "prisma": "^5.9.0",
    "rimraf": "^5.0.5"
  },
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "rimraf ./build && tsc -p tsconfig.json",
    "dev": "NODE_ENV=dev nodemon",
    "start": "node build/index.js",
    "test": "dotenv -e .env.test -- npx prisma migrate reset --force && dotenv -e .env.test -- jest",
    "prisma:reset:dev": "dotenv -e .env.dev -- npx prisma migrate reset --force",
    "lint": "eslint . --ext .ts",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "dotenv -e .env.dev -- npx prisma migrate dev"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
